@model DBLogik.Model.Bruger

<h2>@Model.Navn</h2>
<p>@Model.Mail</p>
<p>Liste over biler:</p>
<ul>
    @foreach(var bil in Model.Biler)
            {
                <li style="color:orange;">@bil.Navn</li>
                <li>@bil.Mærke</li>
                <li>@bil.År</li>
            }
</ul>
<p>Har børn: @Model.HarBørn</p>
<p>@Model.Køn</p>